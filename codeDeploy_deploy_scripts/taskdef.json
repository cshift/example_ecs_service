{
    "executionRoleArn": "arn:aws:iam::${ACCOUNTID}:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "demo-container",
            "image": "${ACCOUNTID}.dkr.ecr.ap-northeast-2.amazonaws.com/test:2",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "dcc-test"
}